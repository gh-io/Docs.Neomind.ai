# Frontend
FRONTEND_PORT=5173

# Backend
APP_NAME=web4app
BACKEND_PORT=8888 or 8080 or 8000 or 50080

# Database
POSTGRES_USER=dev
POSTGRES_PASSWORD=devpass
POSTGRES_DB=devdb
POSTGRES_PORT=5432

PORT=1.1.1.1
COOKIE_SECRET=supersecretvalue

# ==========================================
# NeoMindAI / Brain AI — Dify ENV CONFIG
# ==========================================
        https://updates.dify.ai
# ------------------------------
# Common URLs
# ------------------------------

CONSOLE_API_URL=https://api.neomind.ai
CONSOLE_WEB_URL=https://console.neomind.ai
SERVICE_API_URL=https://api.neomind.ai
TRIGGER_URL=https://api.neomind.ai
APP_API_URL=https://api.neomind.ai
APP_WEB_URL=https://app.neomind.ai

FILES_URL=https://docs.neomind.ai
INTERNAL_FILES_URL=http://api:5000

LANG=en_US.UTF-8
LC_ALL=en_US.UTF-8
PYTHONIOENCODING=utf-8

# ------------------------------
# Server
# ------------------------------
LOG_LEVEL=INFO
LOG_FILE=/app/logs/server.log
LOG_FILE_MAX_SIZE=20
LOG_FILE_BACKUP_COUNT=5
LOG_DATEFORMAT=%Y-%m-%d %H:%M:%S
LOG_TZ=UTC

DEBUG=false
FLASK_DEBUG=false
ENABLE_REQUEST_LOGGING=False

SECRET_KEY=sk-CHANGE-THIS-TO-YOUR-OWN-SECRET-KEY
INIT_PASSWORD=AdminPassword2025

DEPLOY_ENV=PRODUCTION
CHECK_UPDATE_URL=https://docs.dify.ai
OPENAI_API_BASE=https://api.openai.com/v1

MIGRATION_ENABLED=true
FILES_ACCESS_TIMEOUT=300

ACCESS_TOKEN_EXPIRE_MINUTES=60
REFRESH_TOKEN_EXPIRE_DAYS=30

APP_MAX_ACTIVE_REQUESTS=0
APP_MAX_EXECUTION_TIME=1200

# ------------------------------
# Container / Runtime
# ------------------------------
DIFY_BIND_ADDRESS=0.0.0.0
DIFY_PORT=5000
SERVER_WORKER_AMOUNT=2
SERVER_WORKER_CLASS=gevent
SERVER_WORKER_CONNECTIONS=10
GUNICORN_TIMEOUT=360
CELERY_WORKER_CLASS=
CELERY_WORKER_AMOUNT=2
CELERY_AUTO_SCALE=false

# ------------------------------
# Database — POSTGRES (Production)
# ------------------------------

DB_TYPE=postgresql
DB_USERNAME=postgres
DB_PASSWORD=your-strong-password-here
DB_HOST=db_postgres
DB_PORT=5432
DB_DATABASE=dify

SQLALCHEMY_POOL_SIZE=30
SQLALCHEMY_MAX_OVERFLOW=10
SQLALCHEMY_POOL_RECYCLE=3600
SQLALCHEMY_ECHO=false
SQLALCHEMY_POOL_PRE_PING=false
SQLALCHEMY_POOL_USE_LIFO=false
SQLALCHEMY_POOL_TIMEOUT=30

POSTGRES_MAX_CONNECTIONS=200
POSTGRES_SHARED_BUFFERS=256MB
POSTGRES_WORK_MEM=16MB
POSTGRES_MAINTENANCE_WORK_MEM=128MB
POSTGRES_EFFECTIVE_CACHE_SIZE=4096MB
POSTGRES_STATEMENT_TIMEOUT=0
POSTGRES_IDLE_IN_TRANSACTION_SESSION_TIMEOUT=0

# ------------------------------
# Redis
# ------------------------------
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
REDIS_DB=0

CELERY_BROKER_URL=redis://:your-redis-password@redis:6379/1
CELERY_BACKEND=redis
BROKER_USE_SSL=false

# ------------------------------
# CORS
# ------------------------------
WEB_API_CORS_ALLOW_ORIGINS=*
CONSOLE_CORS_ALLOW_ORIGINS=*
COOKIE_DOMAIN=neomind.ai
NEXT_PUBLIC_COOKIE_DOMAIN=1

# ------------------------------
# File Storage (Local - Using OpenDAL FS)
# ------------------------------
STORAGE_TYPE=opendal
OPENDAL_SCHEME=fs
OPENDAL_FS_ROOT=storage

CLICKZETTA_VOLUME_TYPE=user

# S3 (disabled unless you activate it later)
S3_ENDPOINT=
S3_REGION=us-east-1
S3_BUCKET_NAME=
S3_ACCESS_KEY=
S3_SECRET_KEY=
S3_USE_AWS_MANAGED_IAM=false
